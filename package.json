    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "node app.js",
    "dev": "npm run sprites && concurrently npm:watch-css npm:watch-server --raw",
    "test": "jest",
    "css": "sass ./assets/scss/core.scss:./public/core.css ./assets/scss/advanced.scss:./public/advanced.css",
    "watch-server": "npx nodemon app.js --ignore example/",
    "watch-test": "jest --watchAll --reporters=jest-silent-reporter",
    "watch-css": "sass --watch ./assets/scss/core.scss:./public/core.css ./assets/scss/advanced.scss:./public/advanced.css",
    "sprites": "npx svg-sprite --symbol --defs --dest ./public/svg ./assets/svg/*"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ]
  },
