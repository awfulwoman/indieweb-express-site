{{#if opengraph}}
<meta name="twitter:card" content="{{opengraph.twitter.card}}" />
<meta name="twitter:creator" content="{{opengraph.twitter.creator}}" />
<meta property="twitter:url" content="{{opengraph.url}}">
<meta property="twitter:title" content="{{opengraph.title}}">
<meta property="twitter:description" content="{{opengraph.description}}">
<meta property="twitter:image" content="{{opengraph.image.url}}">
<meta property="twitter:image:alt" content="{{opengraph.image.alt}}">
<meta property="og:type" content="{{opengraph.type}}">
<meta property="og:url" content="{{opengraph.url}}" />
<meta property="og:title" content="{{opengraph.title}}" />
<meta property="og:description" content="{{opengraph.description}}" />
<meta property="og:image" content="{{opengraph.image.url}}" />
{{/if}}


